<%- include ('../partials/header') %>
<div class="container">
    <form method="GET">
        <h2>Personal Information </h2>
        <label for="fname">First Name</label> 
        <input type="text" id="fname" name="fname" placeholder="First Name" required>
        <label for="lname">Last Name</label> 
        <input type="text" id="lname" name="lname" placeholder="Last Name" required>
        <label for="email">Email</label> 
        <input type="email" id="email" name="email" placeholder="Enter your Email Id" required>
        <label for="cnum">Contact Number</label> 
        <input type="number" id="cnum" name="cnum" placeholder="Enter your Contact number" required>
        <h2>Contact Information</h2>
        <label for="add">Address</label> 
        <input type="textarea" id="add" name="add" placeholder="Address" required>
        <label for="message">Your Message</label> 
        <input type="textarea" id="message" name="message" placeholder="Enter your message!" required>
    </form>
</div>
<%- include ('../partials/footer') %>